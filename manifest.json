{
  "manifest_version": 2,
  "name": "Fractions - Medium Partner Dashboard",
  "version": "2.0.0",
  "description": "A Better Partner Dashboard for Medium",
  "author": "Can Durmus <@candurmuss>",
  "permissions": ["https://medium.com/"],
  "background": {
    "scripts": ["serviceWorker.js"],
    "persistent": false
  },
  "content_scripts": [],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_title": "Fractions",
    "default_popup": "/popup.html"
  },
  "chromium": {
    "manifest_version": 3
  },
  "firefox": {
    "short_name": "Fractions",
    "browser_specific_setttings": {
      "gecko": {
        "id": "{6a3d8bbc-24ac-4d2c-8b77-c658388e9c8d}",
        "strict_min_version": "57.0"
      }
    }
  }
}
