{
  "manifest_version": 2,
  "name": "Fractions - Medium Partner Dashboard",
  "version": "1.1.1",
  "description": "A Better Partner Dashboard for Medium",
  "author": "Can Durmus <@candurmuss>",
  "permissions": ["https://medium.com/"],
  "background": {
    "scripts": ["serviceWorker.js"],
    "persistent": false
  },
  "content_scripts": [],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_title": "Fractions",
    "default_popup": "/popup.html"
  }
}
